{
  "name": "owl-layout",
  "version": "0.0.0",
  "description": "Overlay for my streams",
  "homepage": "https://bitowl.net",
  "author": {
    "name": "bitowl",
    "email": "bitowl@bitowl.de",
    "url": "https://bitowl.net"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "assetCategories": [
      {
        "name": "eventLogos",
        "title": "Event Logos (64px high)",
        "allowedTypes": [
          "png"
        ]
      }
    ],
    "bundleDependencies": {
      "owl-ldtimer": "^0.0.0",
      "owl-mpc": "^0.0.0",
      "owl-question-box": "^0.0.0",
      "owl-twitch-chat": "^0.0.0"
    },
    "compatibleRange": "~0.9.0",
    "dashboardPanels": [
      {
        "name": "countdown",
        "title": "Setup",
        "width": 2,
        "file": "countdown.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "breakscreen",
        "title": "Break Screen",
        "width": 3,
        "file": "breakscreen.html",
        "headerColor": "#1f2429"
      },
      {
        "name": "breakscreen-preset-delete",
        "title": "Delete Preset?",
        "width": 2,
        "file": "breakscreen-preset-delete.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      },
      {
        "name": "bar-configurator",
        "title": "Bar Configurator",
        "width": 3,
        "file": "bar-configurator.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "bar-content-delete",
        "title": "Delete Bar Content?",
        "width": 2,
        "file": "bar-content-delete.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      },
      {
        "name": "bar-content-add",
        "title": "Add Bar Content?",
        "width": 2,
        "file": "bar-content-add.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      },
      {
        "name": "lowerthird",
        "title": "Lower Third",
        "width": 3,
        "file": "lowerthird.html",
        "headerColor": "#9f9bbd"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "soundCues": [
      {
        "name": "follow-alert"
      },
      {
        "name": "hide-follow-alert"
      },
      {
        "name": "show-question"
      },
      {
        "name": "hide-question"
      },
      {
        "name": "show-lowerthird"
      },
      {
        "name": "hide-lowerthird"
      },
      {
        "name": "show-breakscreen"
      },
      {
        "name": "hide-breakscreen"
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
