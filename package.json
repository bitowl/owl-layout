{
  "name": "owl-layout",
  "version": "0.0.0",
  "description": "Overlay for my streams",
  "homepage": "https://bitowl.net",
  "author": {
    "name": "bitowl",
    "email": "bitowl@bitowl.de",
    "url": "https://bitowl.net"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "assetCategories": [
      {
        "name": "eventLogos",
        "title": "Event Logos (64px high)",
        "allowedTypes": [
          "png"
        ]
      }
    ],
    "compatibleRange": "~0.9.0",
    "dashboardPanels": [
      {
        "name": "countdown",
        "title": "Countdown",
        "width": 2,
        "file": "countdown.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "breakscreen",
        "title": "Break Screen",
        "width": 3,
        "file": "breakscreen.html"
      }, 
      {
        "name": "twitch",
        "title": "Twitch Chat",
        "width": 2,
        "file": "twitch.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "dependencies": {
    "twitch-bot": "^1.2.5"
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
